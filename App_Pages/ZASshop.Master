        <script src="<%# ZASutility.MyUtility.GetSourceWithVersion("/App_Firma/js/vendor/slick.min.js") %>"></script>
        <!--<script src="<%# ZASutility.MyUtility.GetSourceWithVersion("/App_Firma/js/app.min.js") %>"></script>-->
        <script src="<%# ZASutility.MyUtility.GetSourceWithVersion("/App_Firma/js/app.js") %>"></script>
        <script src="<%# ZASutility.MyUtility.GetSourceWithVersion("/App_Firma/js/firm.min.js") %>"></script>
        <link rel="stylesheet" href="<%# ZASutility.MyUtility.GetSourceWithVersion("/App_Firma/js/components/alertifyjs/css/alertify.min.css") %>">
        <link rel="stylesheet" href="<%# ZASutility.MyUtility.GetSourceWithVersion("/App_Firma/js/components/alertifyjs/css/themes/default.min.css") %>">
        <script src="<%# ZASutility.MyUtility.GetSourceWithVersion("/App_Firma/js/components/alertifyjs/alertify.min.js") %>"></script>

        <script>
            var sessionID = '<%: Session.SessionID.ToString() %>';
        </script>
        <script src="<%= ZASutility.MyUtility.GetSourceWithVersion("/App_Firma/js/dropzone.js") %>"></script>

        <div class="hide-control xuser-popup-message" style="display:none;" data-title="Nadpis zpravy">toto je testovaci zprava  v popup okne</div>
        <div class="hide-control xuser-popup-info" style="display:none;">toto je testovaci informace</div>

    </body>
