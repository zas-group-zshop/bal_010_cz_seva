                    TargetControlID="FakeBtnActionForSelectByUser" PopupControlID="SelectStateByUserPanel"
                    DropShadow="true" BackgroundCssClass="modal" BehaviorID="SelectStateByUser_mpe_bhid" />
                <asp:Panel ID="SelectStateByUserPanel" runat="server" CssClass="modal-popup">
                    <!-- pageDesc -->
                    <div class="pageDesc modal-desc">
                        <div class="modal-title"><asp:Literal runat="server" Text='<%# ZASutility.MyUtility.GetLangResource("SelectStateDesc") %>'></asp:Literal></div>
                        <div class="modal-desc">
                            <zas:ZasDropDownList ID="eshop_stat" data-attr-stat runat="server" CssClass="form-change-country form-control" DataTextField="nazev" DataValueField="stat" Source="z_statycis_items/z_statycis_item" />
                        </div>
                    </div>
			        <div class="modal-buttons">
                        <asp:LinkButton runat="server" CssClass="button btn btn-sm btn-default" Text='OK' onclick="SelectState_Click" />
			        </div>
                </asp:Panel>
            </asp:Panel>

            <asp:PlaceHolder ID="phDurAdmin1" runat="server" Visible='<%# (ZASutility.MyUtility.GetSessionBool("spravce") ||
                    ZASutility.MyUtility.IsValueInConfig("ipAddressForLog", "ipAddress", "value", ZASutility.MyUtility.GetSessionRequest(Request.ServerVariables["REMOTE_HOST"])) ||
                    ZASutility.MyUtility.IsValueInConfig("ipAddressForLog", "ipAddress", "value", "All") ||
                    Request.ServerVariables["SERVER_NAME"] == "localhost") ? true : false %>'>
                <div class="log-admin">

                    <asp:UpdatePanel ID="UpdatePanelForAdmin" runat="server" UpdateMode="Conditional">
                        <ContentTemplate>
                            <div class="log-admin-body">
                            
                                <asp:Panel ID="pLog" runat="server">
                                    <div id="pLogBody">
                                        <strong><asp:Literal ID="lAdminLogDescription" runat="server" Text="<%$ Resources: Resource, AdminLogDescription %>" />!!!</strong>
                                        <br /><br />
                                        
                                        <asp:Literal ID="lAdminLogBegin" runat="server" Text="<%$ Resources: Resource, AdminLogBegin %>" />: <asp:Label ID="lAdminLogBeginTime" runat="server" Text="" /><br />

                                        <asp:Panel ID="pLogItems" runat="server" CssClass="pLogItems">

                                        </asp:Panel>

                                        <asp:Literal ID="lAdminLogEnd" runat="server" Text="<%$ Resources: Resource, AdminLogEnd %>" />: <asp:Label ID="lAdminLogEndTime" runat="server" Text="" /><br />
                                        <br />
                                        <strong><asp:Literal ID="lAdminLogNext" runat="server" Text="<%$ Resources: Resource, AdminLogNext %>" />:</strong><br />
                                            <asp:Table ID="tLogItems" runat="server" CssClass="pLogItems table table-condensed table-bordered table-striped">
                                        </asp:Table>
                                    </div>
                                </asp:Panel>

                                <asp:Panel ID="pLogSessionHeader" runat="server" CssClass="collapsePanelHeader" style="margin-top:5px;" Visible="false">
                                    <div style="padding:5px; cursor: pointer; vertical-align: middle;">
                                        <div style="float: left;">
                                            <asp:Literal ID="Literal1" runat="server" Text="<%$ Resources: Resource, AdminLogTitleSessions %>"></asp:Literal></div><div style="float: right; vertical-align: middle;"> 
                                            <asp:Image ID="iLogSessionHeader" runat="server" ImageUrl="~/App_Firma/Images/collapse.jpg" BorderWidth="0" />
                                        </div> 
                                    </div> 
                                </asp:Panel>
                            
                                <asp:Panel ID="pLogSession" runat="server" Visible="false">
                                    <div id="pLogSessionBody" style="border-right:1px solid black;border-bottom:1px solid black;border-left:1px solid black;padding:10px;">
                                        <asp:Table ID="tLogSessionsItems" runat="server" CssClass="pLogSessionsItems">
                                        </asp:Table>
                                    </div>
                                </asp:Panel>

                                <asp:HyperLink ID="ShowAllSessionsLink" runat="server" NavigateUrl="/ShowCommandPage.aspx?show_session=true" Text="Show all sessions" Visible='<%# (ZASutility.MyUtility.GetSessionBool("spravce") || ZASutility.MyUtility.IsValueInConfig("ipAddressForLog", "ipAddress", "value", ZASutility.MyUtility.GetSessionRequest(Request.ServerVariables["REMOTE_HOST"])) || ZASutility.MyUtility.IsValueInConfig("ipAddressForLog", "ipAddress", "value", "All") || Request.ServerVariables["SERVER_NAME"] == "localhost") ? true : false %>' />
                                &nbsp;
                                <asp:LinkButton ID="SelectStateCookieBtn" runat="server" Text='Select state' onclick="SelectStateShowBtn_Click" />
                                &nbsp;
                                <asp:LinkButton ID="ClearStateCookieBtn" runat="server" Text='Clear cookies (state)' onclick="SelectStateClearBtn_Click" />
                            </div>
                        </ContentTemplate>
                    </asp:UpdatePanel>

                    <div class="log-admin-header">

                        <asp:LinkButton CssClass="log-section log-section-1_4" ID="lbClearCache" runat="server" OnClick="lbClearCache_Click" Visible='<%# ZASutility.MyUtility.GetSessionBool("pouzivat_cache") || 1==1 %>'>
                            <i class="fa fa-trash-o" aria-hidden="true"></i> <span class="visible-xs">cache</span><span class="hidden-xs">clear server cache</span>
                        </asp:LinkButton>

                        <asp:LinkButton CssClass="log-section log-section-1_4" ID="lbClearSessions" runat="server" OnClick="lbClearSessions_Click">
                            <i class="fa fa-trash-o" aria-hidden="true"></i> <span class="visible-xs">sessions</span><span class="hidden-xs">clear sessions</span>
                        </asp:LinkButton>

                        <div class="log-section log-section-1_4" title="Velikost stránky">
                            <i class="fa fa-bar-chart" aria-hidden="true"></i> <span><%: (CommunicationUnCompressedLength/1000).ToString() %> kB</span>
                        </div>

                        <div class="log-section log-section-1_4" title="User's count">
                            <i class="fa fa-user" aria-hidden="true"></i> <span><%: Application["users_count"]!=null ? Application["users_count"].ToString() : "n/a" %></span>
                        </div>

                        <div class="log-section log-section-1_2 log-section-clear" title="Čas načtení">
                            <i class="fa fa-clock-o" aria-hidden="true"></i> <span><%: PageCreateDuration!=null ? PageCreateDuration : "n/a" %></span>
                        </div>
                        <a class="log-section log-section-1_2" id="log-show-body" href="">
                            <i class="fa fa-info-circle" aria-hidden="true"></i> <span>Výpis pro správce</span>
                        </a>
                    </div>


                </div>
            </asp:PlaceHolder>
        </form>

