    <%
        try
        {
            string BasketContent = ZASutility.MyUtility.GetSession("basket_items");

            if (!String.IsNullOrEmpty(BasketContent))
            {
                System.Xml.XmlDocument xml = new System.Xml.XmlDocument();
                xml.LoadXml(BasketContent);
                //generuju JSON 
                string BasketItems = String.Empty;
                foreach(System.Xml.XmlNode node in xml.SelectNodes("basket_items/basket_item"))
                {
                    BasketItems += "{" + String.Format("product_id: '{0}', quantity: {1}, value: {2}, currency: '{3}'", ZASutility.MyUtility.GetStringXmlValue(node,"cislo_nomenklatury"), ZASutility.MyUtility.GetStringXmlValue(node,"mnozstvi").Replace(",","."), ZASutility.MyUtility.GetStringXmlValue(node,"cena").Replace(",","."), ZASutility.MyUtility.GetStringXmlValue(node,"id_meny")) + "},";
                }
                if (!String.IsNullOrEmpty(BasketItems))
                {
                    string FirmScript = String.Format("[{0}]", BasketItems);
                    FirmScript = "<script>lhi('SetCart', {products: " + FirmScript + "})</script>";
                    HttpContext.Current.Response.Write(FirmScript);
                }
            }
        }
        catch { }
